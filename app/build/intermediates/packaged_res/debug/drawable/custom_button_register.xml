<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true">
        <shape>
            <solid android:color="@color/white" /> <!-- Ganti dengan warna latar belakang yang diinginkan -->
            <corners android:radius="80dp" /> <!-- Sesuaikan nilai radius dengan keinginan Anda -->
            <stroke
                android:width="6dp"
            android:color="@color/primary" /> <!-- Ganti dengan warna stroke yang diinginkan -->
        </shape>
    </item>
</selector>